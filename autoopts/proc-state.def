type      = '';
prefix    = optproc;
zero-name = none;
mask-type = uint32_t;
no-code;
omit-test-n-set;

/*
 *  If any of these bit assignments change, the minimum version number of
 *  the autoopts interface will also have to be changed.  The result is
 *  incompatible.
 */
cmd[ 0] = longopt;
wht[ 0] = 'Process long style options';

cmd[ 1] = shortopt;
wht[ 1] = 'Process short style "flags"';

cmd[ 2] = errstop;
wht[ 2] = 'Stop on argument errors';

cmd[ 3] = disabledopt;
wht[ 3] = 'Current option is disabled';


cmd[ 4] = no-req-opt;
wht[ 4] = 'no options are required';

cmd[ 5] = num-opt;
wht[ 5] = 'there is a number option';

cmd[ 6] = initdone;
wht[ 6] = 'have inits been done?';

cmd[ 7] = negations;
wht[ 7] = 'any negation options?';

cmd[ 8] = environ;
wht[ 8] = 'check environment?';

cmd[ 9] = no-args;
wht[ 9] = 'Disallow remaining arguments';

cmd[10] = args-req;
wht[10] = 'Require args after options';

cmd[11] = reorder;
wht[11] = 'reorder operands after opts';


cmd[12] = gnuusage;
wht[12] = 'emit usage in GNU style';

cmd[13] = translate;
wht[13] = 'Translate strings in tOptions';

cmd[14] = misuse;
wht[14] = 'no usage on usage error';

cmd[15] = immediate;
wht[15] = 'immediate options active';


cmd[16] = nxlat-opt-cfg;
wht[16] = 'suppress for config only';

cmd[17] = nxlat-opt;
wht[17] = 'suppress xlation always';

cmd[18] = vendor-opt;
wht[18] = 'vendor options active';

cmd[19] = presetting;
wht[19] = 'opt processing in preset state';


mask = {
  m-name = no-xlat;
  m-bit  = nxlat-opt-cfg, nxlat-opt;
};
