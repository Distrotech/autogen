prefix     = optst;
type       = '';
zero-name  = init;
mask-type  = uint32_t;
no-code;
omit-test-n-set;

/*
 *  If any of these bit assignments change, the minimum version number of
 *  the autoopts interface will also have to be changed.  The result is
 *  incompatible.  The first 8 bits are mutable, tho 7 are used.
 */
cmd[ 0] = set;
wht[ 0] = 'Set via the "SET_OPT()" macro';

cmd[ 1] = preset;
wht[ 1] = 'Set via an RC/INI file';

cmd[ 2] = defined;
wht[ 2] = 'Set via a command line option';

cmd[ 3] = reset;
wht[ 3] = 'Reset via command line option';


cmd[ 4] = equivalence;
wht[ 4] = 'selected by equiv\'ed option';

cmd[ 5] = disabled;
wht[ 5] = 'option is in disabled state';

cmd[ 6] = alloc-arg;
wht[ 6] = 'pzOptArg was allocated';


/*
 * The rem must not change
 */
cmd[ 8] = no-init;
wht[ 8] = 'option cannot be preset';

cmd[ 9] = number-opt;
wht[ 9] = 'opt value (flag) is any digit';

cmd[10] = stacked;
wht[10] = 'opt uses optionStackArg proc';

cmd[11] = initenabled;
wht[11] = 'option defaults to enabled';

add-on-text = { ao-file = mask-header;
    ao-text = '#define OPTST_ARG_TYPE_SHIFT 12'; };
cmd[12] = arg-type-1;
wht[12] = 'bit 1 of arg type enum';

cmd[13] = arg-type-2;
wht[13] = 'bit 2 of arg type enum';

cmd[14] = arg-type-3;
wht[14] = 'bit 3 of arg type enum';

cmd[15] = arg-type-4;
wht[15] = 'bit 4 of arg type enum';


cmd[16] = arg-optional;
wht[16] = 'the option arg not required';

cmd[17] = imm;
wht[17] = 'process opt on first pass';

cmd[18] = disable-imm;
wht[18] = 'process disablement immed.';

cmd[19] = omitted;
wht[19] = 'compiled out of program';


cmd[20] = must-set;
wht[20] = 'must be set or pre-set';

cmd[21] = document;
wht[21] = 'opt is for doc only';

cmd[22] = twice;
wht[22] = 'process opt twice - imm + reg';

cmd[23] = disable-twice;
wht[23] = 'process disabled option twice';


cmd[24] = scaled-num;
wht[24] = 'scaled integer value';

cmd[25] = no-command;
wht[25] = 'disable from cmd line';

cmd[26] = deprecated;
wht[26] = 'support is being removed';

cmd[27] = alias;
wht[27] = 'alias for other option';


mask = {
  m-name  = set;
  m-bit   = set, preset, reset, defined;
};

mask = {
  m-name  = mutable;
  m-bit   = set, preset, reset, defined, equivalence, disabled, alloc-arg;
};

mask = {
  m-name  = persistent;
  m-invert;
  m-bit   = 'mutable-mask';
};

mask = {
  m-name  = selected;
  m-bit   = set, defined;
};

mask = {
  m-name  = arg-type;
  m-bit   = arg-type-1, arg-type-2, arg-type-3, arg-type-4;
};

mask = {
  m-name  = no-usage;
  m-bit   = omitted, no-command, deprecated;
};

mask = {
  m-name  = immutable;
  m-bit   = document, omitted;
};

mask = {
  m-name  = do-not-save;
  m-bit   = document, omitted, no-init;
};

mask = {
  m-name  = no-output;
  m-bit   = document, omitted, alias;
};
