# Common definitions
if test -z "$srcdir"; then
    srcdir=`echo "$0" | sed 's,[^/]*$,,'`
    test "$srcdir" = "$0" && srcdir=.
    test -z "$srcdir" && srcdir=.
    test "${VERBOSE+set}" != set && VERBOSE=1
fi
. $srcdir/defs

# Create the files we need in the test environment
cat > $testname.def <<EOF
autogen definitions $testname;
list = { elt = one; };
list = { elt = two; };
list = { elt = three; };
list = { elt = fnord; };
EOF

cat > $testname.tpl <<EOF
<= autogen template test =>
<=_FOR list FROM 1 TO (list _HILIM 1 -)=><=elt=>
<=/list=>
EOF

# this is the output we should expect to see
cat > $testname.out <<EOF
two
three

EOF

$AUTOGEN || exit 1
cmp -s $testname.test $testname.out || failure unexpected output

# end of forfrom.test
