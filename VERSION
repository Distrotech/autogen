# Maintainer:     Bruce Korb <korbb@datadesign.com>
#          by:    Bruce Korb <korbb@datadesign.com>
# ----------------------------------------------------------------------

AG_MAJOR_VERSION=4
AG_MINOR_VERSION=4
AG_REVISION=$AG_MAJOR_VERSION.$AG_MINOR_VERSION
AG_PATCHLEVEL=.2
AG_VERSION=$AG_REVISION$AG_PATCHLEVEL

MAINTAINER='Bruce Korb <korbb@datadesign.com>'

# For automake
#
VERSION=$AG_VERSION
PACKAGE=autogen

# For my sanity!
#
echo "configuring $PACKAGE $VERSION"

# VERSION ends here
