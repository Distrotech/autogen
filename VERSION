#          by:    Bruce Korb <korbb@datadesign.com>
# ----------------------------------------------------------------------

AG_MAJOR_VERSION=4
AG_MINOR_VERSION=2
AG_MICRO_VERSION=-
AG_PATCHLEVEL=1
AG_VERSION=$AG_MAJOR_VERSION.$AG_MINOR_VERSION$AG_MICRO_VERSION$AG_PATCHLEVEL

MAINTAINER='Bruce Korb <korbb@datadesign.com>'

# For automake
VERSION=$AG_VERSION
PACKAGE=autogen

# For my sanity!
echo "configuring $PACKAGE $VERSION"

# VERSION ends here
